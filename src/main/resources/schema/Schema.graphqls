type Content {
  contentId: Int!
  contentTitle: String!
  contentBody: String
  commentList: [Comment]
}

type Comment {
  commentId: Int!
  commentBody: String
}

type Query {
  contentList: [Content]
  getContent(contentId: Int): Content
}

extend type Query {
  commentList: [Comment]
}

type Mutation {
  writeContent(title: String!, body: String): Int
}

schema {
  query: Query
  mutation: Mutation
}